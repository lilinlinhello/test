<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false" />
	</settings>
	<typeAliases>
		<typeAlias type="com.yunda.app.entity.pojo.User" alias="user"/>
		<typeAlias type="com.yunda.app.entity.pojo.Role" alias="role"/>
		<typeAlias type="com.yunda.app.entity.pojo.SysUser" alias="sysUser"/>
		<typeAlias type="com.yunda.app.entity.pojo.Menu" alias="menu"/>
		<typeAlias type="com.yunda.app.entity.pojo.Resource" alias="resource"/>
		<typeAlias type="com.yunda.app.entity.pojo.Auth" alias="auth"/>
		<typeAlias type="com.yunda.app.entity.pojo.RoleUserMap" alias="roleUserMap"/>
		<typeAlias type="com.yunda.app.entity.pojo.FileLoad" alias="fileload"/>
		<typeAlias type="com.yunda.app.entity.pojo.JcdfLog" alias="jcdflog"/>
		<typeAlias type="com.yunda.app.entity.pojo.Dictionarys" alias="dictionary"/>
		<typeAlias type="com.yunda.gps.common.location.bean.BaseAreaLocal" alias="location"/>
		<typeAlias type="com.yunda.gps.common.fileUD.bean.Documents" alias="documents"/>
		<typeAlias type="com.yunda.gps.gpsEquipRegister.bean.GpsEquipRegister" alias="gpsEquipRegister"/>
		<typeAlias type="com.yunda.gps.vehicleCategory.bean.VehicleCategory" alias="vehicleCategory"/>
		<typeAlias type="com.yunda.gps.vehicleCategory.bean.VehicleCategoryVo" alias="vehicleCategoryVo"/>
		<typeAlias type="com.yunda.gps.vehicleMaintain.pojo.VehicleMaintain" alias="vehicleMaintain"/>
		<typeAlias type="com.yunda.gps.vehicleMaintain.vo.VehicleMaintainVo" alias="vehicleMaintainVo"/>
		<typeAlias type="com.yunda.gps.vehicleRecord.bean.VehicleRecord" alias="vehicleRecord"/>
		<typeAlias type="com.yunda.gps.vehicleMaintain.pojo.VideoLine" alias="videoLine"/>
		<typeAlias type="com.yunda.gps.monitor.pojo.AlarmLogging" alias="alarmLogging"/>
		<typeAlias type="com.yunda.gps.driver.bean.DriverManager" alias="driverManager"/>
		<typeAlias type="com.yunda.gps.monitor.pojo.LineMileage" alias="lineMileage"/>
		<typeAlias type="com.yunda.inter.outsideInterface.bean.SynVehicle" alias="synVehicle"/>
		<typeAlias type="com.yunda.app.entity.pojo.AppReg" alias="appReg"/>
		<typeAlias type="com.yunda.gps.smsTemplate.pojo.SMSTemplateVo" alias="smsTemplateVo"/>
		<typeAlias type="com.yunda.gps.smsTemplate.pojo.SmsSendVo" alias="smsSendVo"/>
	</typeAliases>
	
	<!-- 分页插件(通过在mybatis底层拦截查询动作，动态解析组装sql语句实现动态分页功能) -->
	<plugins>
		<plugin interceptor="com.yunda.app.util.platform.filter.PageInterceptor"></plugin>
	</plugins>
</configuration>
