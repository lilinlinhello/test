<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yunda.gps.vehicleMaintain.mapper.GpsRawDataMapper">
	<select id='findRecentGpsData' resultType="map" parameterType="string">
		select
		conver_longitude as longitude,
		conver_latitude as latitude,
		concat(command_code,'H') as command_code,
		date_format(gps_create_at,'%Y-%c-%d %H:%i:%s') as gps_create_at,
		date_format(create_at,'%Y-%c-%d %H:%i:%s') as create_at
		from ${tableName}
		where vehicle_num=#{vehicleId} order by create_at desc limit 10
	</select>
</mapper>